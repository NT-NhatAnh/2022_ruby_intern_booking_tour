<%# <div class="colorlib-loader"></div> %>
<div id="page">
  <aside id="colorlib-hero">
    <div class="flexslider">
      <ul class="slides">
        <li style="background-image: url(<%= asset_path("img_bg_1.jpg") %>);">
          <div class="overlay"></div>
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                <div class="slider-text-inner text-center">
                  <h1><%= t ".amazing_maldivest_tour" %></h1>
                </div>
              </div>
            </div>
          </div>
        </li>
        <li style="background-image: url(<%= asset_path("img_bg_2.jpg") %>);">
          <div class="overlay"></div>
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                <div class="slider-text-inner text-center">
                  <h1><%= t ".from_greece_to_spain" %></h1>
                </div>
              </div>
            </div>
          </div>
        </li>
        <li style="background-image: url(<%= asset_path("img_bg_5.jpg") %>);">
          <div class="overlay"></div>
          <div class="container-fluids">
            <div class="row">
              <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                <div class="slider-text-inner text-center">
                  <h2><%= t ".explore_our_most_travel_agency" %></h2>
                  <h1><%= t ".our_travel_agency" %></h1>
                </div>
              </div>
            </div>
          </div>
        </li>
        <li style="background-image: url(<%= asset_path("img_bg_4.jpg") %>);">
          <div class="overlay"></div>
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                <div class="slider-text-inner text-center">
                  <h2><%= t ".experience_the" %></h2>
                  <h1><%= t ".best_trip_ever" %></h1>
                </div>
              </div>
            </div>
          </div>
        </li>
      </ul>
    </div>
  </aside>
  <div id="colorlib-reservation">
    <!-- <div class="container"> -->
    <div class="row">
      <div class="search-wrap">
        <div class="container">
          <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#flight"><i class="flaticon-plane"></i> <%= t(".search_tours")%></a></li>
          </ul>
        </div>
        <div class="tab-content">
          <div id="flight" class="tab-pane fade in active">
            <form method="post" class="colorlib-form">
              <div class="row">
                <div class="col-md-3">
                  <div class="form-group">
                    <label for="date"><%= t(".tour_name")%></label>
                    <div class="form-field">
                      <input type="text" id="location" class="form-control" placeholder="<%= t(".search_Location")%>">
                    </div>
                  </div>
                </div>
                <div class="col-md-3">
                  <div class="form-group">
                    <label for="guests"><%= t(".category")%></label>
                    <div class="form-field">
                      <i class="icon icon-arrow-down3"></i>
                      <select name="people" id="people" class="form-control">
                      </select>
                    </div>
                  </div>
                </div>
                <div class="col-md-2">
                  <div class="form-group">
                    <label for="date"><%= t(".start_date")%></label>
                    <div class="form-field">
                      <i class="icon icon-calendar2"></i>
                      <input type="text" id="date" class="form-control date" placeholder="<%= t(".check-in_date")%>">
                    </div>
                  </div>
                </div>
                <div class="col-md-2">
                  <div class="form-group">
                    <label for="date"><%= t(".end_date")%></label>
                    <div class="form-field">
                      <i class="icon icon-calendar2"></i>
                      <input type="text" id="date" class="form-control date" placeholder="<%= t(".check-out_date")%>">
                    </div>
                  </div>
                </div>
                <div class="col-md-2">
                  <input type="submit" name="submit" id="submit" value="Find" class="btn btn-primary btn-block">
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div id="colorlib-services">
  <div class="container">
    <div class="row no-gutters">
      <div class="col-md-3 animate-box text-center aside-stretch">
        <div class="services">
          <span class="icon">
            <i class="flaticon-around"></i>
          </span>
          <h3><%= t ".amazing_travel" %></h3>
          <p><%= t ".travel_description" %></p>
        </div>
      </div>
      <div class="col-md-3 animate-box text-center">
        <div class="services">
          <span class="icon">
            <i class="flaticon-boat"></i>
          </span>
          <h3><%= t ".our_cruises" %></h3>
          <p><%= t ".travel_description" %></p>
        </div>
      </div>
      <div class="col-md-3 animate-box text-center">
        <div class="services">
          <span class="icon">
            <i class="flaticon-car"></i>
          </span>
          <h3><%= t ".book_your_trip" %></h3>
          <p><%= t ".travel_description" %></p>
        </div>
      </div>
      <div class="col-md-3 animate-box text-center">
        <div class="services">
          <span class="icon">
            <i class="flaticon-postcard"></i>
          </span>
          <h3><%= t ".nice_support" %></h3>
          <p><%= t ".travel_description" %></p>
        </div>
      </div>
    </div>
  </div>
</div>
