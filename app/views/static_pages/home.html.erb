<div class="colorlib-loader"></div>
<div id="page">
  <aside id="colorlib-hero">
    <div class="flexslider">
      <ul class="slides">
        <li class="slides_1">
          <div class="overlay"></div>
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                <div class="slider-text-inner text-center">
                  <h1><%= t ".amazing_maldivest_tour" %></h1>
                </div>
              </div>
            </div>
          </div>
        </li>
        <li class="slides_2">
          <div class="overlay"></div>
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                <div class="slider-text-inner text-center">
                  <h1><%= t ".from_greece_to_spain" %></h1>
                </div>
              </div>
            </div>
          </div>
        </li>
        <li class="slides_3">
          <div class="overlay"></div>
          <div class="container-fluids">
            <div class="row">
              <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                <div class="slider-text-inner text-center">
                  <h2><%= t ".explore_our_most_travel_agency" %></h2>
                  <h1><%= t ".our_travel_agency" %></h1>
                </div>
              </div>
            </div>
          </div>
        </li>
        <li class="slides_4">
          <div class="overlay"></div>
          <div class="container-fluid">
            <div class="row">
              <div class="col-md-6 col-md-offset-3 col-sm-12 col-xs-12 slider-text">
                <div class="slider-text-inner text-center">
                  <h2><%= t ".experience_the" %></h2>
                  <h1><%= t ".best_trip_ever" %></h1>
                </div>
              </div>
            </div>
          </div>
        </li>
      </ul>
    </div>
  </aside>
</div>
<div id="colorlib-services">
  <div class="container">
    <div class="row no-gutters">
      <div class="col-md-3 animate-box text-center aside-stretch">
        <div class="services">
          <span class="icon">
            <i class="flaticon-around">
              <span class="iconify" data-icon="material-symbols:auto-awesome-outline" data-width="50" data-height="50"></span>
            </i>
          </span>
          <h3><%= t ".amazing_travel" %></h3>
          <p><%= t ".travel_description" %></p>
        </div>
      </div>
      <div class="col-md-3 animate-box text-center">
        <div class="services">
          <span class="icon">
            <i class="flaticon-boat">
              <span class="iconify" data-icon="ic:outline-tour" data-width="50" data-height="50"></span>
            </i>
          </span>
          <h3><%= t ".our_cruises" %></h3>
          <p><%= t ".travel_description" %></p>
        </div>
      </div>
      <div class="col-md-3 animate-box text-center">
        <div class="services">
          <span class="icon">
            <i class="flaticon-car">
              <span class="iconify" data-icon="icon-park-outline:tour-bus" data-width="50" data-height="50"></span>
            </i>
          </span>
          <h3><%= t ".book_your_trip" %></h3>
          <p><%= t ".travel_description" %></p>
        </div>
      </div>
      <div class="col-md-3 animate-box text-center">
        <div class="services">
          <span class="icon">
